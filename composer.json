  "description": "Handlr is a lightweight PHP middleware style framework.",
  "version": "0.1.0",
  "minimum-stability": "stable",
  "type": "library",
  "license": "MIT",
  "keywords": [ "framework", "handlr" ],
  "homepage": "https://github.com/phillipsharring/handlr",
  "authors": [
    {
      "name": "Phillip Harrington",
      "email": "phillip@phillipharrington.comngton.com"
    }
  ],
  "require": {
    "php": ">=8.4",
    "ext-pdo": "*",
    "psr/log": "^3.0",
    "vlucas/phpdotenv": "^5.6",
    "ramsey/uuid": "^4.7",
    "adbario/php-dot-notation": "^3.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
